":"codeVoice"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Import the "},{"type":"codeVoice","code":"OneEntry"},{"text":" module:","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["import Foundation","import SwiftUI","import OneEntry","","\/\/ ..."],"type":"codeListing"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Configure a "},{"type":"codeVoice","code":"OneEntryCore"},{"type":"text","text":" shared instance, before using SDK methods. The "},{"type":"codeVoice","code":"application(_:didFinishLaunchingWithOptions:)"},{"text":" method is best suited for this.","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["let token = \"...\"","let url = \"https:\/\/sample.oneentry.cloud\"","let credential = OneEntryTokenCredential(token: token)","","OneEntryCore.initializeApp(url, credentials: credential)"]}]}],"type":"orderedList"},{"type":"heading","anchor":"Initialization-with-certificates","level":3,"text":"Initialization with certificates"},{"type":"aside","style":"note","content":[{"type":"paragraph","inlineContent":[{"text":"iOS Sdk OneEntry uses .p12(PKCS#12) certificates for authorization. Such a certificate can be generated independently from the ","type":"text"},{"code":"key","type":"codeVoice"},{"type":"text","text":" & "},{"type":"codeVoice","code":"certificate"},{"text":" pair or use the one provided by OneEntry","type":"text"}]}],"name":"Note"},{"type":"paragraph","inlineContent":[{"text":"In any case, you need to download the archive with certificates. You can do this in the project settings, in the ","type":"text"},{"type":"codeVoice","code":"Application Certificates"},{"type":"text","text":" tab"}]},{"inlineContent":[{"type":"text","text":"The following files will be inside:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".cert"},{"text":" - Certificate, encrypted in base64","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":".csr"},{"type":"text","text":" - Certificate request"}]}]},{"content":[{"inlineContent":[{"code":".key","type":"codeVoice"},{"text":" - Key, encrypted in base64","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":".p12"},{"text":" - The ","type":"text"},{"type":"codeVoice","code":"Certificate"},{"type":"text","text":" & "},{"type":"codeVoice","code":"Key"},{"text":" bundle, encrypted in PKCS#12","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"heading","level":4,"text":"Using the built-in certificate","anchor":"Using-the-built-in-certificate"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Move the .p12 file to the project, make sure it is included in Target Membership"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Initialize the application. The "},{"type":"codeVoice","code":"application(_:didFinishLaunchingWithOptions:)"},{"type":"text","text":" method is best suited for this."}]}]}],"type":"orderedList"},{"name":"Note","type":"aside","style":"note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"By default, OneEntry certificates do not have a password"}]}]},{"syntax":"swift","code":["let url = \"https:\/\/sample.oneentry.cloud\"","let name = \"swift_certificate\"","let credential = OneEntryCertificateCredential(name: name)","","OneEntryCore.initializeApp(domain, credentials: credential)"],"type":"codeListing"},{"type":"heading","text":"Generation of your .p12 certificate","level":4,"anchor":"Generation-of-your-p12-certificate"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"To do this, you will need a key("},{"type":"codeVoice","code":".key"},{"text":") and certificate(","type":"text"},{"code":".cert","type":"codeVoice"},{"type":"text","text":") file, download them"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Generate .p12